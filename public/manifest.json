{
  "name": "Coding Advice",
  "description" : "Random coding advice when opens a new Chrome tab.",
  "version": "1.0.0",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "coding-advice.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
